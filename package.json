{
    "name": "meloncholy-games-website",
    "version": "1.0.0",
    "description": "Website and Subdomains for Meloncholy Games",
    "scripts": {
        "clean": "rm -rf cdk.out coverage dist",
        "checkFormat": "prettier --check \"src/**/*.ts*\" \"tst/**/*.ts*\" \"infrastructure/**/*.ts\"",
        "format": "prettier --write \"src/**/*.ts*\" \"tst/**/*.ts*\" \"infrastructure/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts*\" \"tst/**/*.ts*\" \"infrastructure/**/**/*.ts\"",
        "types": "tsc --noEmit",
        "test": "jest --silent",
        "build": "vite build",
        "start": "vite",
        "bootstrap": "cdk bootstrap",
        "deploy": "cdk deploy --require-approval=never --all"
    },
    "author": "Max Rosoff",
    "license": "MIT",
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@mui/icons-material": "^5.16.0",
        "@mui/lab": "^5.0.0-alpha.171",
        "@mui/material": "^5.16.0",
        "aws-cdk-lib": "^2.160.0",
        "axios": "^1.7.2",
        "clsx": "latest",
        "framer-motion": "^11.2.13",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.24.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@testing-library/react": "^16.0.1",
        "@tsconfig/node18": "^18.2.4",
        "@types/jest": "^29.5.13",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/luxon": "^3.4.2",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.2",
        "aws-cdk": "^2.160.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.12",
        "globals": "^15.10.0",
        "jest": "^29.7.0",
        "jsonwebtoken": "^9.0.2",
        "prettier": "^3.3.3",
        "sass": "^1.79.4",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.0",
        "typescript-eslint": "^8.8.0",
        "vite": "^5.4.8",
        "vite-plugin-electron": "^0.28.8",
        "vite-plugin-node-polyfills": "^0.22.0"
    }
}
