{
    "name": "meloncholy-games-website",
    "version": "1.0.0",
    "description": "Website and Subdomains for Meloncholy Games",
    "scripts": {
        "clean": "rm -rf cdk.out coverage dist",
        "check-format": "prettier --check \"src/**/*.ts*\" \"tst/**/*.ts*\"",
        "format": "prettier --write \"src/**/*.ts*\" \"tst/**/*.ts*\"",
        "lint": "eslint \"src/**/*.ts*\" \"tst/**/*.ts*\"",
        "types": "tsc --noEmit",
        "test": "jest --silent",
        "build": "vite build",
        "start": "vite",
        "bootstrap": "cdk bootstrap",
        "deploy": "cdk deploy --require-approval=never --all"
    },
    "author": "Max Rosoff",
    "license": "MIT",
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
